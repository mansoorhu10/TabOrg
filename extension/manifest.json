{
"name": "TabOrg",
"version": "2.0",
"description": "Tab Organizer Chrome Extension",
"icons": {
    "16": "src/icons/TabOrg Logo - 16.png",
    "48": "src/icons/TabOrg Logo - 48.png",
    "128": "src/icons/TabOrg Logo - 128.png"
},
"action": {
    "default_title": "Organize your Tabs!",
    "default_popup": "public/popup.html"
},
"permissions": [
    "activeTab",
    "tabs",
    "storage"
],
"host_permissions": [
    "<all_urls>"
],
"manifest_version": 3,
"minimum_chrome_version": "95",
"background": {
    "service_worker": "src/background.js"
}


}