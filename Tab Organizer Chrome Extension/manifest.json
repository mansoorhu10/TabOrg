{
"name": "TabOrg",
"version": "1.0",
"description": "Tab Organizer Chrome Extension",
"browser_action": {
    "default_title": "Organize your Tabs!",
    "default_popup": "popup.html"
},
"permissions": [
    "tabs",
    "<all_urls>"
],
"background": {
    "scripts": ["background.js"],
    "persistent": false
},
"manifest_version": 2

}